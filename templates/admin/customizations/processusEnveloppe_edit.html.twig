{% extends '@EasyAdmin/default/edit.html.twig' %}

{% block head_stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('admin_pe') }}
{% endblock %}

{% block main %}

    <input type="text" class="form-control" id="ge-filter" placeholder="Filtrer les GEs">

    {% block entity_form %}
        {{ form_start(form) }}
<div class="row">
    <div class="col-12">
      {{ dump(form) }}
        <div id="processus-enveloppe"></div>
        <div id="gamme-enveloppes"></div>
    </div>
</div
        {{ form_end(form) }}

    {% endblock entity_form %}
{% endblock %}


{% block body_javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('admin_pe') }}
{% endblock %}